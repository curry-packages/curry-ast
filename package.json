{
    "name": "curry-ast",
    "version": "3.0.0",
    "author": "Kai-Oliver Prott <stu126209@mail.uni-kiel.de>",
    "synopsis": "Libaries to represent Curry programs",
    "category": [ "Metaprogramming" ],
    "compilerCompatibility": {
        "pakcs": ">= 3.8.0, < 4.0.0",
        "kics2": ">= 3.3.0, < 4.0.0"
    },
    "dependencies": {
        "base"          : ">= 3.0.0",
        "currypath"     : ">= 3.0.0, < 4.0.0",
        "directory"     : ">= 3.0.0, < 4.0.0",
        "filepath"      : ">= 3.0.0, < 4.0.0",
        "frontend-exec" : ">= 3.3.0, < 4.0.0",
        "read-legacy"   : ">= 3.0.0, < 4.0.0"
    },
    "exportedModules": [ "Curry.Types",
                         "Curry.Files",
                         "Curry.Ident",
                         "Curry.Position",
                         "Curry.Span" ,
                         "Curry.SpanInfo" ],
    "source": {
        "git": "https://github.com/curry-packages/curry-ast.git",
        "tag": "$version"
    }
}
